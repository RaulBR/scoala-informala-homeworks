package ro.scii.iothred;

public class VoteCommitteeThread implements Runnable{
	
	
	
	public void run() {
		boolean b=false;
		while(b==false){
		try{
			
			Thread.sleep(5000);
			VoteCommittee vote = new VoteCommittee();
			vote.addCandidate("Muttley");
			vote.addCandidate("Mr. Bean");
			vote.addCandidate("Spirt Mona");
					
					vote.print();
		} catch(Exception e){
			
		}
	
	
		
	}
	}
}
