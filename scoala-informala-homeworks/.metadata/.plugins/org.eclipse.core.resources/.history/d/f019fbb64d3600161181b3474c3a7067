package ro.scii.iothred;

public class VoteThred {


	private class VoteThread  {

	
		public VoteThred(VoteBallot vote) {
			
			
					new Thread() {
						public void run() {
							try {
								Thread.sleep(50);
							} catch (InterruptedException e) {
								e.printStackTrace();
							}
						box.addVote(vote);
						};
					}.start();
				
			}

		}
}
	